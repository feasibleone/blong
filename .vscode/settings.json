{
    "files.associations": {
        "rush.json": "jsonc"
    },
    "search.exclude": {
        "**/.rush/temp/": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true
    },
    "workbench.sideBar.location": "right",
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.words": [
        "adaptername",
        "Ambrym",
        "Anders",
        "apidevtools",
        "Bislama",
        "bitsyntax",
        "blong",
        "clusterrole",
        "clusterrolebinding",
        "clusterrolebindings",
        "clusterroles",
        "codegen",
        "configmap",
        "daemonset",
        "dapr",
        "datetime",
        "Diebold",
        "dukpt",
        "endregion",
        "ESOCKETTIMEDOUT",
        "fastify",
        "feasibleone",
        "Hejlsberg",
        "kalinkrustev",
        "keyblock",
        "kopi",
        "kubeconfig",
        "mdns",
        "mlek",
        "mlsk",
        "networkpolicies",
        "npmjs",
        "OpenAPIV2",
        "OpenAPIV3_1",
        "parentspanid",
        "payshield",
        "persistentvolume",
        "persistentvolumeclaim",
        "persistentvolumeclaims",
        "persistentvolumes",
        "pino",
        "pnpmfile",
        "rdkafka",
        "realmname",
        "replicaset",
        "replicasets",
        "rolebinding",
        "rolebindings",
        "rushstack",
        "spanid",
        "statefulset",
        "statefulsets",
        "traceid",
        "typebox",
        "ulidx",
        "unannounce",
        "unixtime",
        "unstate"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "pnpm-lock.yaml",
        "rush.json"
    ]
}
